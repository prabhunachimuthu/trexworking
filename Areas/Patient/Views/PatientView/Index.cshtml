            <div class="login">
                <div class="row">
                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <div class="col s12">
                            <div class="input-field">
                                @Html.HiddenFor(model => model.ruserid)
                                @Html.HiddenFor(model => model.rtype)
                                @Html.HiddenFor(model => model.rpage)
                                @Html.TextBoxFor(model => model.UserId, new { @class = "validate", @style = "background-color:white;color:black;", @placeholder = "Enter User Name" })
