                                @*<input type="password" class="validate" placeholder="PASSWORD" required>*@
                            </div>
                            @*<a href=""><h6>Forgot Password ?</h6></a>*@
                            <button style="background-color:white;" class="button-default" type="submit">Login</button>
                        </div>
